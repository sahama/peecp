<!DOCTYPE html>
<html lang="{{request.locale_name}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="peecp (peereesook control panel)">
    <meta name="author" content="peereesook team">
    <link rel="shortcut icon" href="{{request.static_url('peecp:static/peereesook_96x96.png')}}">

    <title>Peereesook Control Panel</title>
  </head>
  <body>
  <style>
      body {
  padding-top: 50px;
}
  </style>

    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>

            <a class="navbar-brand" rel="home" href="{{ request.route_path('home') }}" title="{{ _('title') }}">
                <img style="max-width:40px; margin-top: -7px;"
                     src="{{ request.static_url('peecp:static/peereesook_96x96.png') }}">
            </a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container">

           {% block content %}
                <p>No content</p>
            {% endblock content %}


    </div><!-- /.container -->


  </body>
</html>
